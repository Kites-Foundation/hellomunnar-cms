{
  "kind": "collectionType",
  "collectionName": "features",
  "info": {
    "name": "feature",
    "description": "List of all the features available in destinations"
  },
  "options": {
    "increments": true,
    "timestamps": false,
    "populateCreatorFields": false,
    "draftAndPublish": false,
    "privateAttributes": [
      "id"
    ]
  },
  "attributes": {
    "featureName": {
      "type": "string",
      "required": true
    },
    "featureIcon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "featureUrl": {
      "type": "string",
      "private": false,
      "required": true,
      "default": "#"
    }
  }
}
