{
  "kind": "collectionType",
  "collectionName": "things_to_dos",
  "info": {
    "name": "thingsToDo",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "route": {
      "type": "component",
      "repeatable": false,
      "component": "common.route",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "location": {
      "type": "component",
      "repeatable": true,
      "component": "collection-type-components.location"
    },
    "openTime": {
      "type": "time"
    },
    "closingTime": {
      "type": "time"
    },
    "difficultyLevel": {
      "type": "enumeration",
      "enum": [
        "Challenging",
        "Adventure",
        "Easy"
      ],
      "required": true
    },
    "price": {
      "type": "string"
    },
    "booking": {
      "type": "component",
      "repeatable": false,
      "component": "collection-type-components.booking"
    },
    "reviews": {
      "via": "thingsToDo",
      "collection": "review",
      "model": "review"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Trekking",
        "Boating",
        "SiteSeeing",
        "Camping",
        "Cycling",
        "Wildlife",
        "RockClimbing",
        "Photography",
        "Landscape",
        "SunriseSunset",
        "TeaPlantationFarming",
        "Festivals",
        "Heritage",
        "ThemePark",
        "Shopping",
        "Fooding",
        "Adventure",
        "Safari"
      ],
      "required": true
    },
    "challenges": {
      "collection": "challenge"
    }
  }
}
