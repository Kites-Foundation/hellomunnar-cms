{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "review",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "published": {
      "type": "boolean",
      "default": false,
      "private": true,
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "reviews"
    },
    "destination": {
      "via": "reviews",
      "collection": "destination"
    }
  }
}
